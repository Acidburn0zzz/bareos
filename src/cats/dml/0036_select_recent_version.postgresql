SELECT DISTINCT ON (File.Name,
                    PathId) JobTDate,
                   JobId,
                   FileId,
                   FileIndex,
                   PathId,
                   File.Name,
                   LStat,
                   MD5
FROM FILE
JOIN Job USING (JobId)
WHERE JobId IN (%s) BY File.Name,
                       PathId,
                       JobTDate DESC
